      "text": {
        "size": 16,
        "text": "Swipe for more",
        "style": "normal",
        "colour": "#ffffff",
        "text_type": "separator",
        "uppercase": false
      }
    },
    {
      "content_type": 3,
      "padding": 45,
      "order": 20
